{
  "name": "askdotmd",
  "displayName": "ask_dot_md",
  "version": "1.0.0",
  "description": "chatgpt in your vscode",
  "keywords": [],
  "author": "Gabe DiFiore",
  "license": "GPL-3.0-only",
  "repository": {},
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:askdotmd.sendRequest"
  ],
  "contributes": {
    "commands": [
      {
        "command": "askdotmd.sendRequest",
        "title": "Send Request to LLM API"
      }
    ],
    "keybindings": [
      {
        "command": "askdotmd.sendRequest",
        "key": "ctrl+shift+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run webpack",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "webpack": "webpack --mode production"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "@types/vscode": "^1.89.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "openai": "^4.47.1"
  }
}
